{
    "name": "Replace content example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "tabs",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "js": [
                "lib/mark.es6.min.js",
                "replace.js"
            ]
        }
    ]
}
