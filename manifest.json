{
    "name": "Replacer - word annotator",
    "description": "Sample extension for annotating words",
    "version": "1.1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "tabs",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "css": [
                "css/main.css"
            ],
            "js": [
                "lib/mark.es6.min.js",
                "js/replace.js"
            ]
        }
    ]
}
